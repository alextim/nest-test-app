{
  "name": "nest-test-app",
  "version": "0.0.1",
  "description": "bla bla",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist && npm i && npm i -g @nestjs/cli && npm i -g cross-env && npm i -g typeorm && npm i -g dotenv",
    "build": "nest build && npm run typeorm:seed",
    "mongo-start": "docker run -d -p 27017:27017 --name mongo-dev mongo:5",
    "mongo-stop": "docker rm -f mongo-dev",
    "start": "node dist/src/main",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/src/main",
    "start:staging": "cross-env NODE_ENV=staging node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test,seed}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"seed/**/*.ts\"",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "typeorm:seed": "npm run typeorm migration:run -- -d ./seed/typeorm-seed.config.ts",
    "typeorm:run-migrations": "cross-env NODE_ENV=development npm run typeorm migration:run -- -d ./typeorm.config.ts",
    "typeorm:generate-migration": "cross-var npm run typeorm -- -d ./typeorm.config.ts migration:generate ./migrations/$npm_config_name",
    "typeorm:create-migration": "cross-var npm run typeorm -- migration:create ./migrations/$npm_config_name",
    "typeorm:revert-migration": "npm run typeorm -- -d ./typeorm.config.ts migration:revert",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "clean:all": "rimraf ./**/dist ./**/.pnpm-debug.log ./**/.turbo ./**/coverage ./**/pnpm-lock.yaml ./**/node_modules"
  },
  "dependencies": {
    "@nestjs/common": "^9.3.9",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.3.9",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.3.9",
    "@nestjs/serve-static": "^3.0.1",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/terminus": "^9.2.1",
    "@nestjs/typeorm": "^9.0.1",
    "@rewiko/crud": "^5.1.12",
    "@rewiko/crud-typeorm": "^5.1.12",
    "@sentry/node": "^7.40.0",
    "@types/etag": "^1.8.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "connect-pg-simple": "^8.0.0",
    "cron-validate": "^1.4.5",
    "dotenv": "^16.0.3",
    "etag": "^1.8.1",
    "express-rate-limit": "^6.7.0",
    "express-session": "^1.17.3",
    "handlebars": "^4.7.7",
    "helmet": "^6.0.1",
    "humanize-duration": "^3.28.0",
    "multer": "1.4.5-lts.1",
    "nestjs-pino": "^3.1.2",
    "nodemailer": "^6.9.1",
    "passport": "^0.6.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.9.0",
    "pino": "^8.11.0",
    "pino-http": "^8.3.3",
    "rand-token": "^1.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.2.0",
    "rxjs": "^7.8.0",
    "typeorm": "^0.3.12",
    "typeorm-extension": "^2.5.2",
    "typeorm-naming-strategies": "^4.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.9",
    "@types/express": "^4.17.17",
    "@types/express-rate-limit": "^6.0.0",
    "@types/express-session": "^1.17.6",
    "@types/helmet": "^4.0.0",
    "@types/jest": "29.4.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.14.6",
    "@types/nodemailer": "^6.4.7",
    "@types/passport-facebook": "^2.1.11",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-local": "^1.0.35",
    "@types/sonic-boom": "^2.1.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "cross-env": "^7.0.3",
    "cross-var": "^1.1.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.4.3",
    "pino-pretty": "^9.4.0",
    "prettier": "^2.8.4",
    "puppeteer": "^19.7.2",
    "sonic-boom": "^3.2.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.2",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
