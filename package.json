{
  "name": "nestjs-mongo-starter",
  "version": "0.0.1",
  "description": "bla bla",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "mongo-start": "docker run -d -p 27017:27017 --name mongo-dev mongo:5",
    "mongo-stop": "docker rm -f mongo-dev",
    "start": "node dist/main",
    "start:dev": "SET NODE_ENV=development&& nest start --watch",
    "start:debug": "SET NODE_ENV=development&& nest start --debug --watch",
    "start:prod": "SET NODE_ENV=production&& node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "typeorm:run-migrations": "npm run typeorm migration:run -- -d ./typeorm.config.ts",
    "typeorm:generate-migration": "cross-var npm run typeorm -- -d ./typeorm.config.ts migration:generate ./migrations/$npm_config_name",
    "typeorm:create-migration": "cross-var npm run typeorm -- migration:create ./migrations/$npm_config_name",
    "typeorm:revert-migration": "npm run typeorm -- -d ./typeorm.config.ts migration:revert",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.1",
    "@nestjs/mapped-types": "^1.2.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/terminus": "^9.1.4",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjsx/crud": "5.0.0-alpha.3",
    "@nestjsx/crud-typeorm": "5.0.0-alpha.3",
    "@sentry/node": "^7.28.1",
    "@types/etag": "^1.8.1",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "connect-pg-simple": "^8.0.0",
    "dotenv": "^16.0.3",
    "etag": "^1.8.1",
    "express-rate-limit": "^6.7.0",
    "express-session": "^1.17.3",
    "googleapis": "^110.0.0",
    "handlebars": "^4.7.7",
    "helmet": "^6.0.1",
    "humanize-duration": "^3.27.3",
    "multer": "1.4.5-lts.1",
    "nestjs-pino": "^3.1.1",
    "nodemailer": "^6.8.0",
    "passport": "^0.6.0",
    "passport-facebook": "^3.0.0",
    "passport-facebook-token": "^4.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.8.0",
    "pino": "^8.8.0",
    "pino-http": "^8.3.0",
    "rand-token": "^1.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.0",
    "typeorm": "^0.3.11",
    "typeorm-naming-strategies": "^4.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.7",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.2.1",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.15",
    "@types/express-rate-limit": "^6.0.0",
    "@types/express-session": "^1.17.5",
    "@types/helmet": "^4.0.0",
    "@types/jest": "29.2.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.18",
    "@types/nodemailer": "^6.4.7",
    "@types/passport-facebook": "^2.1.11",
    "@types/passport-facebook-token": "^0.4.35",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-local": "^1.0.34",
    "@types/sonic-boom": "^2.1.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "cross-var": "^1.1.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.3.1",
    "pino-pretty": "^9.1.1",
    "prettier": "^2.8.1",
    "sonic-boom": "^3.2.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.9.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
